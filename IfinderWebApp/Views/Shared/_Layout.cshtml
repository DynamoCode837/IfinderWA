    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - IfinderWebApp</title>

    <!-- Bootstrap CSS -->
    <link href="~/Template1/css/bootstrap.min.css" rel="stylesheet" />
