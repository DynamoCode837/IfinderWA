<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - IfinderWebApp</title>

    <!-- Required meta tags always come first -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="x-ua-compatible" content="ie=edge">

    <!-- Bootstrap CSS -->
    <link href="~/Template3/Bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Template3/Bootstrap/dist/css/bootstrap-grid.css" rel="stylesheet" />
    <link href="~/Template3/Bootstrap/dist/css/bootstrap-reboot.css" rel="stylesheet" />

    <!-- Main Styles CSS -->
    <link href="~/Template3/css/main.min.css" rel="stylesheet" />
    <link href="~/Template3/css/fonts.min.css" rel="stylesheet" />


</head>
<body>

    <!-- Fixed Sidebar Left -->

